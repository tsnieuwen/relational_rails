<h1>Athletes</h1>

<% @athletes.each do |athlete| %>
  <% if @team.id == athlete.team_id %>
    <p><a href="/athletes/<%= athlete.id %>"><%= athlete.name %></a></p>
  <% end %>
<% end %><br>

<form action="/teams/<%= @team.id %>/athletes/new" method="get">
  <button type="submit">Create Athlete</button>
</form>
